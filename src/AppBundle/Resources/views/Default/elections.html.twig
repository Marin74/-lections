{% extends 'base.html.twig' %}

{% block title %}
	{{ "title_app_name"|trans({"%app_name%": app_name, "%title%": "history"|trans}) }}
{% endblock %}

{% block body %}
<div class="container">
    <div class="col-md-6">
        {% for electionType in electionTypes %}
            <h3>
                {{ electionType.name }}
            </h3>
            <ul>
				{% for election in electionType.elections %}
					<li><a href="{{ url("app_election", {"id": election.id}) }}">{{ election.name }}</a></li>
				{% endfor %}
            </ul>
        {% endfor %}
    </div>
</div>	
{% endblock %}
{% block css %}
{% endblock %}
{% block js %}
{% endblock %}